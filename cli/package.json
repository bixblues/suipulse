{
  "name": "@suipulse/cli",
  "version": "0.1.0",
  "description": "CLI tool for SuiPulse SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "suipulse": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "version": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "keywords": [
    "sui",
    "blockchain",
    "cli",
    "suipulse"
  ],
  "author": "SuiPulse Team",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui.js": "^0.42.0",
    "@suipulse/sdk": "0.1.0",
    "@types/js-yaml": "^4.0.9",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
