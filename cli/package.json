{
  "name": "@suipulse/cli",
  "version": "0.1.0-cliv1",
  "description": "CLI tool for SuiPulse SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "suipulse": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "sui",
    "blockchain",
    "cli",
    "suipulse"
  ],
  "author": "SuiPulse Team",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui.js": "^0.42.0",
    "@suipulse/sdk": "0.1.0",
    "@types/js-yaml": "^4.0.9",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "globals": "^16.1.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bixblues/suipulse/cli"
  },
  "bugs": {
    "url": "https://github.com/bixblues/suipulse/issues"
  },
  "homepage": "https://github.com/bixblues/suipulse#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
